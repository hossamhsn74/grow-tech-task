[run]
source = .

[report]
exclude_lines =
    pragma: no cover
    def __str__
    def __repr__
    if self.debug
    if settings.DEBUG
    if __name__ == .__main__.:
    
omit =
    */venv/*
    */migrations/*
    */fixtures/*
    */tests/*
    manage.py
    */__init__.py
    */wsgi.py
    */asgi.py
    */settings.py
    */urls.py
    */apps.py
    */consts.py
